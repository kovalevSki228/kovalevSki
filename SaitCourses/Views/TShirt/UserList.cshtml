@model  IEnumerable<SaitCourses.Models.User>

<h2>List user</h2>

<table>
    @foreach(var user in Model)
    {
        <tr>
            <td>@user.UserName</td>
            <td>
                <a class="btn btn-sm btn-outline-primary" asp-action="Edit" asp-route-userid="@user.Id">Prova dostupa</a>
            </td>
        </tr>
    }
</table>